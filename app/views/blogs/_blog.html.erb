<!-- Content -->
<section>
  <header id="blogheader">
    <h1><%= blog.title %></h1>
  </header>
  <p>Published on: <%= blog.created_at.strftime('%B %d, %Y') %></p>
  <p class="center">    
    <img class="blog-image" src= <%= blog.image_url %>>
  </p>
  <p>
    <%= blog.body.html_safe %>
  </p>
</section>
